<?xml version="1.0" encoding="windows-1251"?>

<std_view SPXML-FORM="//base1/base1_std_view.xmd">
	<id>positions_of_division</id>
	<name VALUE="Штатные должности по подразделению" lang-en:VALUE="Positions of division" lang-ua:VALUE="Штатні посади по підрозділу"/>
	<parent_id>positions</parent_id>

	<drop_all_dyn_filters>1</drop_all_dyn_filters>

	<dyn_filters>
		<dyn_filter>
			<id>division_id</id>
			<is_auto>1</is_auto>
		</dyn_filter>
	</dyn_filters>

	<fields>
		<field>
			<id>name</id>
			<col_title VALUE="##UiText.objects.position##"/>
		</field>
		<field>
			<id>multi_employee_id</id>
		</field>
		<field>
			<col_title>+</col_title>
			<title_expr>ArrayCount( multi_employee_id ) != 0 ? ArrayCount( multi_employee_id ) : ''</title_expr>
			<align>center</align>
			<data_type>integer</data_type>
			<width>3zr</width>
		</field>
		<field>
			<id>sum_employment_percent</id>
		</field>
	</fields>
</std_view>
