<?xml version="1.0" encoding="windows-1251"?>
<SPXML-FORM>

<replica_settings>
	<scenarios>
		<scenario MULTIPLE="1" PRIMARY-KEY="id">
			<id TYPE="string"/>

			<db_name TYPE="string"/>

			<server_address TYPE="string"/>
			<login TYPE="string"/>
			<password TYPE="string"/>
			<compress TYPE="bool" NOT-NULL="1"/>

			<stat>
				<local_last_date TYPE="date"/>
				<remote_last_date TYPE="date"/>
			</stat>
		</scenario>
	</scenarios>
</replica_settings>


</SPXML-FORM>



