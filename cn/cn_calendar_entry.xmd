<?xml version="1.0" encoding="windows-1251"?>
<SPXML-FORM BASE-OBJECT="event" TITLE="##UiText.objects.calendar_entry##" CODE-LIB="1" IMAGE-URL="//base_pict/calendar_entry.ico" IMAGE-URL-EXPR="get_image_url()" SCREEN-FORM-URL="//base2/base2_event.xms">

<calendar_entry PRIMARY-DISP-NAME="type_id.ForeignDispName + ' ' + StrDate( date, true, false )">
