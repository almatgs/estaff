	<is_delegate TYPE="bool" NULL-FALSE="1" TITLE="##UiText.fields.is_delegate##"/>
	
	<outlook_eid TYPE="string" INDEXED="1" FT-IGNORE="1"/>
	<lotus_eid TYPE="string" INDEXED="1" FT-IGNORE="1"/>
	<google_eid TYPE="string" INDEXED="1" FT-IGNORE="1"/>

	<entry_pass_request_eid TYPE="string" FT-IGNORE="1" TITLE="Номер заявки на пропуск" lang-en:TITLE="Entry Pass ID" lang-ua:TITLE="Номер заявки на пропуск"/>
</calendar_entry>



</SPXML-FORM>