	</IF>
</LINE>
<BR/>
<LINE>
	<CHECK SOURCE="Ps.restrict_view_vacancies_with_recruit_type" AUTO-LABEL="1"/>
	<ITEM TYPE="voc_elem_selector" SOURCE="Ps.vacancy_recruit_type_id" LABEL-SIDE="left"/>
</LINE>
<DELIM/>

<LINE MULTILINE="1">
	<CHECK SOURCE="Ps.prohibit_view_other_user_candidates" TITLE="Запрещен просмотр кандидатов других пользователей" READ-ONLY-EXPR="Ps.allow_all" lang-en:TITLE="Prohibit viewing candidates of other users" lang-ua:TITLE="Заборонений перегляд кандидатів інших користувачів"/>
