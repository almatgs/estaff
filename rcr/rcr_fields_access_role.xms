	<CHECK SOURCE="Ps.prohibit_open_other_group_candidates" TITLE="Запрещен просмотр содержимого карточек кандидатов других групп" READ-ONLY-EXPR="Ps.allow_all" lang-en:TITLE="Prohibit opening cards of candidates of other groups" lang-ua:TITLE="Заборонений перегляд вмісту карток кандидатів інших груп"/>
</LINE>

<LINE MULTILINE="1">
	<CHECK SOURCE="Ps.prohibit_view_other_spot_candidates_with_state" AUTO-LABEL="1" MULTILINE="1"/>
