</LINE>

<LINE MULTILINE="1">
	<IF EXPR="global_settings.is_agency">
		<CHECK SOURCE="Ps.prohibit_view_vacancy_revenue" TITLE="Запрещен просмотр выручки агентства по вакансии" READ-ONLY-EXPR="Ps.allow_all" lang-en:TITLE="Prohibit viewing agency revenue of vacancies" lang-ua:TITLE="Заборонений перегляд виручки агентства по вакансії"/>
