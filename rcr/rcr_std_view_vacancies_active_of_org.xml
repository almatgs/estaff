<?xml version="1.0" encoding="windows-1251"?>
<std_view SPXML-FORM="//base1/base1_std_view.xmd">
	<id>vacancies_active_of_org</id>
	<parent_id>vacancies</parent_id>
	<name VALUE="Открытые вакансии по организации" lang-en:VALUE="Active vacancies of company" lang-ua:VALUE="Відкриті вакансії по організації"/>
	<image_url>//base_pict/vacancy.ico</image_url>
	<order_index>1</order_index>

	<is_card_page>1</is_card_page>
	<card_object_type_id>org</card_object_type_id>
	<card_page_title VALUE="##UiText.titles.active_vacancies##"/>
	<exist_req_expr>global_settings.is_agency</exist_req_expr>

	<static_filters>
		<static_filter>
			<id>is_active</id>
			<value>true</value>
		</static_filter>
	</static_filters>

	<drop_all_dyn_filters>1</drop_all_dyn_filters>

	<dyn_filters>
		<dyn_filter>
			<id>org_id</id>
			<is_auto>1</is_auto>
		</dyn_filter>
	</dyn_filters>
</std_view>

