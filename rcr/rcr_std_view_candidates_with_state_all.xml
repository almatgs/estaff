<?xml version="1.0" encoding="windows-1251"?>
<std_view SPXML-FORM="//base1/base1_std_view.xmd">
	<id>candidates_with_state_all</id>
	<name VALUE="Кандидаты по статусу - все" lang-en:VALUE="Candidates by state - all" lang-ua:VALUE="Кандидати за статусом - всі"/>
	<parent_id>candidates</parent_id>

	<fields>
		<field>
			<id>state_date</id>
			<use_time>0</use_time>
			<col_title VALUE="##UiText.fields.date##"/>
		</field>
		<field>
			<id>fullname</id>
		</field>
		<field>
			<id>state_id</id>
			<bk_color_expr>get_state_bk_color()</bk_color_expr>
		</field>
		<field>
			<id>main_vacancy_id</id>
			<width>40%</width>
		</field>
		<field>
			<id>main_vacancy_org_id</id>
			<exist_req_expr>global_settings.is_agency</exist_req_expr>
			<width>30%</width>
		</field>
		<field>
			<id>main_vacancy_division_id</id>
			<exist_req_expr>! global_settings.is_agency</exist_req_expr>
			<width>30%</width>
		</field>
		<field__>
			<id>source_id</id>
			<col_title VALUE="##UiText.fields.source##"/>
			<text_color>90,120,0</text_color>
		</field__>
		<field>
			<id>last_job_org_name</id>
			<text_color>90,0,120</text_color>
			<width>30%</width>
		</field>
		<field__>
			<id>last_comment</id>
		</field__>
		<field>
			<id>user_id</id>
		</field>
	</fields>
</std_view>

