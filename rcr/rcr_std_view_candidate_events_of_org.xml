<?xml version="1.0" encoding="windows-1251"?>
<std_view SPXML-FORM="//base1/base1_std_view.xmd">
	<id>candidate_events_of_org</id>
	<parent_id>events</parent_id>
	<name VALUE="События по кандидатам по клиенту" lang-en:VALUE="Candidate Events for Company" lang-ua:VALUE="Події по кандидатам по клієнту"/>
	<image_url>x-app://base_pict/event.ico</image_url>
	<order_index>4</order_index>
	<exist_req_expr>global_settings.is_agency</exist_req_expr>

	<is_card_page>1</is_card_page>
	<card_object_type_id>org</card_object_type_id>
	<card_page_title VALUE="##UiText.sections.candidate_events##"/>

	<drop_all_dyn_filters>1</drop_all_dyn_filters>

	<dyn_filters>
		<dyn_filter>
			<id>vacancy_org_id</id>
			<is_auto>1</is_auto>
		</dyn_filter>

		<dyn_filter>
			<id>date</id>
			<use_range>1</use_range>
		</dyn_filter>

		<dyn_filter>
			<id>type_id</id>
		</dyn_filter>
	</dyn_filters>
</std_view>

