							</template>
							<template>
								<mail_template_id>first_call_arrange_2</mail_template_id>
							</template>
							<template>
								<mail_template_id>interview_invite</mail_template_id>
							</template>
							<template>
								<mail_template_id>interview_invite_2</mail_template_id>
							</template>
						</templates>
					</core_action>
				</core_actions>
			</occurrence>
