			</COLUMNS>

			<IF EXPR="fields_usage.use_object_field( 'person', 'country_id', 'common' )">
				<ITEM TYPE="voc_elem_selector" SOURCE="Ps.country_id" AUTO-LABEL="1"/>
			</IF>
			<IF EXPR="fields_usage.use_object_field( 'person', 'location_id', 'common' )">
