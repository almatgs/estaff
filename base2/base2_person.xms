			<IF EXPR="lib_app2.AppFeatureEnabled( 'rr_recruit' )">
				<LABEL/>
				<CHECK SOURCE="Ps.is_hiring_manager" AUTO-LABEL="1" WIDTH="auto"/>
			</IF>

			<IF EXPR="Ps.ChildExists( 'is_rr_person' ) && global_settings.use_rr_person_flag">
