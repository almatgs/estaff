<access_role_base SAMPLE="1">
	<id TYPE="string"/>
	<name TYPE="string"/>

	<allow_all TYPE="bool" NULL-FALSE="1"/>
	<read_only TYPE="bool" NULL-FALSE="1"/>

	<allow_change_trash TYPE="bool" NULL-FALSE="1"/>
	<allow_change_global_settings TYPE="bool" NULL-FALSE="1"/>
	<allow_change_users TYPE="bool" NULL-FALSE="1"/>
	<allow_view_rev_deals TYPE="bool" NULL-FALSE="1"/>
	<allow_view_exclusive_objects TYPE="bool" NULL-FALSE="1" TITLE="Разрешен просмотр объектов с ограниченным доступом" lang-en:TITLE="Allow viewing exclusive access objects" lang-ua:TITLE="Дозволений перегляд об'єктів з обмеженим доступом"/>

	<prohibit_delete_all TYPE="bool" NULL-FALSE="1" TITLE="Запрещено удаление любых объектов" lang-en:TITLE="Prohibit deleting any object" lang-ua:TITLE="Заборонено видалення будь-яких об'єктів"/>
	<prohibit_change_object_user TYPE="bool" NULL-FALSE="1" TITLE="Запрещена смена пользователя в любой карточке" lang-en:TITLE="Prohibit changing user for any object"/>
	<prohibit_change_object_group TYPE="bool" NULL-FALSE="1" TITLE="Запрещена смена группы в любой карточке" lang-en:TITLE="Prohibit changing group for any object"/>
	<prohibit_list_copy TYPE="bool" NULL-FALSE="1" TITLE="Запрещено копирование и экспорт списков на экране" lang-en:TITLE="Prohibit mass copy and export of lists" lang-ua:TITLE="Заборонено копіювання та експорт списків на екрані"/>
	<prohibit_change_orgs TYPE="bool" NULL-FALSE="1"/>
	<prohibit_view_other_group_orgs TYPE="bool" NULL-FALSE="1"/>
	<prohibit_view_other_group_positions TYPE="bool" NULL-FALSE="1"/>
	<prohibit_view_other_user_events TYPE="bool" NOT-NULL="1"/>
	<prohibit_view_other_group_events TYPE="bool" NOT-NULL="1"/>
	<prohibit_change_other_group_events TYPE="bool" NOT-NULL="1" TITLE="Запрещено редактирование событий других групп" lang-en:TITLE="Prohibit editing events of other groups" lang-ua:TITLE="Заборонити редагування календарів інших груп"/>
	<prohibit_add_past_events TYPE="bool" NOT-NULL="1" TITLE="Запрещено добавлять события задним числом" lang-en:TITLE="Prohibit adding events manually" lang-ua:TITLE="Заборонено додавати події заднім числом"/>

	<prohibit_view_agreements TYPE="bool" NULL-FALSE="1" TITLE="Запрещен просмотр договоров" lang-en:TITLE="Prohibit viewing agreements" lang-ua:TITLE="Заборонено перегляд договорів"/>
	<prohibit_view_expenses TYPE="bool" NULL-FALSE="1" TITLE="Запрещен просмотр затрат на подбор" lang-en:TITLE="Prohibit viewing expenses" lang-ua:TITLE="Заборонено перегляд витрат на підбір"/>

	<prohibit_view_other_group_training_groups TYPE="bool" NULL-FALSE="1" TITLE="Запрещен просмотр учебных групп других групп пользователей" lang-en:TITLE="Prohibit viewing training groups of other user's group" lang-ua:TITLE="Заборонено перегляд навчальних груп інших груп користувачів"/>

	<t1 TYPE="bool" EXPR="
			if ( allow_all )
			{
				read_only = false;

				for ( elem in allow_all.Parent )
				{
					if ( StrBegins( elem.Name, 'allow_' ) )
						elem.Value = true;
					else if ( StrBegins( elem.Name, 'prohibit_' ) )
						elem.Value = false;
					else if ( StrBegins( elem.Name, 'restrict_' ) )
						elem.Value = false;
				}
			}
	" EXPR-SHOW-ERRORS="1"/>
</access_role_base>



