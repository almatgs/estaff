<object_state_base SAMPLE="1">
	<id TYPE="string"/>
	<name TYPE="string"/>
	<short_name TYPE="string"/>
	<is_active TYPE="bool" NOT-NULL="1" DEFAULT="true"/>
	<order_index TYPE="integer"/>

	<text_color TYPE="string"/>
	<bk_color TYPE="string"/>
	<image_url TYPE="string"/>

	<is_standalone TYPE="bool" NULL-FALSE="1" TITLE="Не отражается записью в личном деле" lang-en:TITLE="Does not match any record" lang-ua:TITLE="Не відображається записом в особовій справі"/>
	<event_type_id TYPE="string" FOREIGN-ARRAY="event_types" TITLE="Базовое событие" lang-en:TITLE="Base event types" lang-ua:TITLE="Базове подія"/>
	<event_occurrence_id TYPE="string" FOREIGN-ARRAY="base2_common.event_occurrences" TITLE="Признак завершения" lang-en:TITLE="Completion" lang-ua:TITLE="Ознака завершення"/>

	<INHERIT TYPE="state_attr_base"/>

	<get_short_name PROPERTY="1" EXPR="
			if ( short_name.HasValue )
				return short_name;
			else
				return name;
	"/>

	<get_image_url PROPERTY="1" EXPR="
			if ( image_url.HasValue )
				return image_url;
			else
				return '//base_pict/event.ico';
	"/>

	<get_rollback_state PROPERTY="1" EXPR="
			statesVoc = DefaultDb.GetOptCatalog( lib_base.object_name_to_catalog_name( Name ) );
			
			rollbackOccurrenceID = '';

			rollbackState = ArrayOptFind( statesVoc, 'event_type_id == ' + CodeLiteral( event_type_id ) + ' && event_occurrence_id == ' + CodeLiteral( rollbackOccurrenceID ) );
			return rollbackState;
	"/>
</object_state_base>



