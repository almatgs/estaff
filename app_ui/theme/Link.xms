<?xml version="1.0" encoding="windows-1251"?>
<SPXML-SCREEN>

<LABEL NAME="LINK" SAMPLE="1" STATES="hover,pressed" CURSOR="pointer" TEXT-COLOR-EXPR="
	switch ( Item.State )
	{
		case 'normal':
			return '';

		case 'hover':
			return AppUi.gThemeData.colors.primary;

		case 'pressed':
			return AppUi.gThemeData.colors.dark;
	}
"/>




</SPXML-SCREEN>









